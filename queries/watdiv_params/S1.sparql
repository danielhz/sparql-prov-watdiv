PREFIX gr: <http://purl.org/goodrelations/>
PREFIX sorg: <http://schema.org/>
SELECT ?v2 WHERE {
  ?v0 gr:includes ?v1 .
  ?v2 gr:offers ?v0 .
  ?v0 gr:price ?v3 .
  ?v0 gr:serialNumber ?v4 .
  ?v0 gr:validFrom ?v5 .
  ?v0 gr:validThrough ?v6 .
  ?v0 sorg:eligibleQuantity ?v7 .
  ?v0 sorg:eligibleRegion ?v8 .
  ?v0 sorg:priceValidUntil ?v9 .
}
