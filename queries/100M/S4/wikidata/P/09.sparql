PREFIX foafp: <http://example.org/p/xmlns.com/foaf/>
PREFIX mop: <http://example.org/p/purl.org/ontology/mo/>
PREFIX sorgp: <http://example.org/p/schema.org/>
PREFIX foafps: <http://example.org/ps/xmlns.com/foaf/>
PREFIX mops: <http://example.org/ps/purl.org/ontology/mo/>
PREFIX sorgps: <http://example.org/ps/schema.org/>
PREFIX wsdbm: <http://db.uwaterloo.ca/~galuc/wsdbm/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
SELECT ?v0 ?v2 ?v3
       ?sum_product_1_source
       ?sum_product_2_source
       ?sum_product_3_source
       ?sum_product_4_source
       ?sum_product
       ?sum
WHERE {
  ?v0 foafp:age ?sum_product_1_source . ?sum_product_1_source foafps:age wsdbm:AgeGroup3 .
  ?v0 foafp:familyName ?sum_product_2_source . ?sum_product_2_source foafps:familyName ?v2 .
  ?v3 mop:artist ?sum_product_3_source . ?sum_product_3_source mops:artist ?v0 .
  ?v0 sorgp:nationality ?sum_product_4_source . ?sum_product_4_source sorgps:nationality wsdbm:Country1 .
  BIND (URI(CONCAT("http://example.org/sum_product/",
                   "?s_1=", ENCODE_FOR_URI(xsd:string(?v0)),
                   "&s_2=", ENCODE_FOR_URI(xsd:string(?v2)),
                   "&s_3=", ENCODE_FOR_URI(xsd:string(?v3))))
        AS ?sum_product )
  BIND (URI(CONCAT("http://example.org/sum/",
                   "?s_1=", ENCODE_FOR_URI(xsd:string(?v0)),
                   "&s_2=", ENCODE_FOR_URI(xsd:string(?v2)),
                   "&s_3=", ENCODE_FOR_URI(xsd:string(?v3))))
        AS ?sum )
}
