PREFIX gn: <http://www.geonames.org/ontology#>
PREFIX sorg: <http://schema.org/>
PREFIX wsdbm: <http://db.uwaterloo.ca/~galuc/wsdbm/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?v0 ?v1 ?v3 WHERE {
  ?sum_product_1_source rdf:subject wsdbm:City46 ; rdf:predicate gn:parentCountry ; rdf:object ?v3 .
  ?sum_product_2_source rdf:subject ?v0 ; rdf:predicate sorg:nationality ; rdf:object ?v3 .
  OPTIONAL { ?sum_product_3_source rdf:subject ?v0 ; rdf:predicate sorg:jobTitle ; rdf:object ?v1 . }
}
