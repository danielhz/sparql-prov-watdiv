PREFIX gnp: <http://example.org/p/www.geonames.org/ontology#>
PREFIX sorgp: <http://example.org/p/schema.org/>
PREFIX wsdbmp: <http://example.org/p/db.uwaterloo.ca/~galuc/wsdbm/>
PREFIX gnps: <http://example.org/ps/www.geonames.org/ontology#>
PREFIX sorgps: <http://example.org/ps/schema.org/>
PREFIX wsdbmps: <http://example.org/ps/db.uwaterloo.ca/~galuc/wsdbm/>
PREFIX wsdbm: <http://db.uwaterloo.ca/~galuc/wsdbm/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
SELECT ?v1 ?v2
       ?sum_product_1_source
       ?sum_product_2_source
       ?sum_product_3_source
       ?sum_product
       ?sum
WHERE {
  wsdbm:City237 gnp:parentCountry ?sum_product_1_source . ?sum_product_1_source gnps:parentCountry ?v1 .
  ?v2 wsdbmp:likes ?sum_product_2_source . ?sum_product_2_source wsdbmps:likes wsdbm:Product0 .
  ?v2 sorgp:nationality ?sum_product_3_source . ?sum_product_3_source sorgps:nationality ?v1 .
  BIND (URI(CONCAT("http://example.org/sum_product/",
                   "?s_1=", ENCODE_FOR_URI(xsd:string(?v1)),
                   "&s_2=", ENCODE_FOR_URI(xsd:string(?v2))))
        AS ?sum_product )
  BIND (URI(CONCAT("http://example.org/sum/",
                   "?s_1=", ENCODE_FOR_URI(xsd:string(?v1)),
                   "&s_2=", ENCODE_FOR_URI(xsd:string(?v2))))
        AS ?sum )
}
