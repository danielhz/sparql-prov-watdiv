PREFIX wsdbm: <http://db.uwaterloo.ca/~galuc/wsdbm/>
PREFIX sorg: <http://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?v0
WHERE {
  GRAPH ?sum_product_1_source { ?v0 rdf:type wsdbm:ProductCategory3 . }
  GRAPH ?sum_product_2_source { ?v0 sorg:description ?v2 . }
  GRAPH ?sum_product_3_source { ?v0 sorg:keywords ?v3 . }
  GRAPH ?sum_product_4_source { ?v0 sorg:language wsdbm:Language0 . }
}
